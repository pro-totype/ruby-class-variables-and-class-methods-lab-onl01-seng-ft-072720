class Song 
  
  def initialize(song,artists,genre)
    @song = song
    @artists = artists
    @genre = genre
  end

  def name
    
  end

  def artists=(artists)
    @artists = artists
  end
  
  def artists
    @artists
  end
  
  def genre=(genre)
    @genre = genre
  end
  
  def genre
    @genre
  end
  
  
  
end